{
    "name": "kanagama/laravel-collection-deduplicate",
    "description": "Collection クラスを拡張して、重複を排除する deduplicate() メソッドを追加します",
    "keywords": [
        "laravel",
        "collection",
        "deduplicate"
    ],
    "authors": [
        {
            "name": "kazuma nagama",
            "email": "k.nagama0632@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^8.0 | ^9.0 | ^10.0",
        "php": "^7.4 | ^8.0 | ^8.1 | ^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 | ^10.0",
        "orchestra/testbench": "*",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Kanagama\\LaravelCollectionDeduplicate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "Kanagama\\LaravelCollectionDeduplicate\\Tests\\": "tests/"
        }
    },
    "exclude": [
        "tests/",
        ".docker/",
        ".github/",
        "*.md"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kanagama\\LaravelCollectionDeduplicate\\CollectionMethodServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
